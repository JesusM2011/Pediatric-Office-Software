<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #262626;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cse360p1.InputRecommendations">
   <children>
      <TextArea fx:id="recommendations_field" layoutX="29.0" layoutY="62.0" prefHeight="252.0" prefWidth="541.0" promptText="Enter findings and recommendations" style="-fx-control-inner-background: #6c727a;" />
      <Label fx:id="recommendations_label" layoutX="14.0" layoutY="14.0" text="Input recommendations for:" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="110.0" layoutY="339.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: #075de8;" text="Go Back" textFill="WHITE" />
      <Button fx:id="submit" layoutX="388.0" layoutY="339.0" mnemonicParsing="false" onAction="#submitRecommendations" style="-fx-background-color: #075de8;" text="Submit" textFill="WHITE" />
   </children>
</AnchorPane>
